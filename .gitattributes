# Auto detect text files and normalize line endings to LF
* text=auto

# Unity YAML files
*.unity merge=unityyamlmerge eol=lf
*.prefab merge=unityyamlmerge eol=lf
*.asset merge=unityyamlmerge eol=lf
*.scene merge=unityyamlmerge eol=lf
*.meta merge=unityyamlmerge eol=lf
# C# files
*.cs diff=csharp eol=lf
# Binary files
*.bin binary
*.dll binary
*.so binary
*.exe binary
*.o binary
*.obj binary
# Image files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.psd binary
*.tga binary
*.tif binary
*.tiff binary
*.bmp binary
# Audio files
*.mp3 binary
*.wav binary
*.ogg binary
*.aif binary
*.aiff binary
# Video files
*.mp4 binary
*.mov binary
*.avi binary
# 3D model files
*.fbx binary
*.blend binary
*.max binary
*.gltf binary
*.glb binary
# Package files
*.unitypackage binary
*.apk binary
*.aab binary
*.unity filter=lfs diff=lfs merge=lfs -text
*.asset filter=lfs diff=lfs merge=lfs -text
